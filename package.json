{
  "name": "wasm-masm-x86-editor",
  "version": "1.0.0",
  "description": "Portable Microsoft Assembly Language Editor with compilation, linking and x86 executable execution",
  "main": "./build/electron-main.js",
  "scripts": {
    "build": "gulp",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider gulp watch",
    "build-electron": "gulp electron",
    "build-installer": "electron-builder",
    "deploy": "electron-builder build -mwl --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/istareatscreens/wasm-masm-x86-editor"
  },
  "keywords": [
    "genetic",
    "algorithm",
    "wasm",
    "maze"
  ],
  "author": "istareatscreens",
  "bugs": {
    "url": "https://github.com/istareatscreens/GeneticMazeSolver/issues"
  },
  "homepage": "https://github.com/istareatscreens/GeneticMazeSolver#readme",
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^9.8.8",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "browser-sync": "^3.0.4",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.11",
    "del": "^6.0.0",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-better-rollup": "^4.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-livereload": "^4.0.2",
    "gulp-postcss": "^9.0.0",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^6.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.0.1",
    "loader-utils": "^3.3.1",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.5.3",
    "sass": "^1.89.0",
    "ts-loader": "^8.4.0",
    "typescript": "^4.9.5",
    "webpack": "^5.99.8",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^5.7.3",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "codemirror": "^5.65.19",
    "electron-updater": "^4.3.5",
    "eol": "^0.9.1",
    "file-saver": "^2.0.5",
    "gulp-htmlmin": "^5.0.1",
    "react": "^17.0.2",
    "react-codemirror2": "^7.3.0",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.3",
    "react-onclickoutside": "^6.13.2",
    "use-debounce": "^5.2.1"
  },
  "build": {
    "appId": "com.isas.wasm-masm-x86-editor",
    "productName": "WASM MASM x86 Editor",
    "extends": null,
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico"
    },
    "files": [
      "./build/*",
      "package.json"
    ],
    "nsis": {
      "uninstallDisplayName": "WASM MASM x86 Editor",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "artifactName": "${productName}-${version}-osx.${ext}",
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "icon.icns"
    },
    "linux": {
      "artifactName": "${productName}-${version}-${arch}-linux.${ext}",
      "maintainer": "istareatscreens",
      "target": [
        "tar.gz",
        "deb",
        "rpm",
        "pacman"
      ]
    }
  }
}